    "framer-motion": "10.16.4",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ui": "*"
  },
  "devDependencies": {
    "@chakra-ui/cli": "2.4.1",
    "@types/node": "18.18.9",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@vitejs/plugin-react": "4.1.1",
    "eslint": "8.53.0",
    "eslint-config-react-ts": "*",
    "prettier": "3.0.3",
    "tsconfig-react": "*",
